import java.util.Scanner;

public class SelectionStatements {
	
	public static void main (String[] args) {
		
		System.out.println("Please enter a number:");
		
		Scanner scannerObjec = new Scanner(System.in);
		int number = scannerObjec.nextInt();
		if(number% 2 == 0) {
			System.out.println("The condition evaluated to true");
		}
		
		else {
			System.out.println("The condition evaluated to false");
		}
		
		System.out.println("Please enter a number:");
		int grade1 = scannerObjec.nextInt();

	
		System.out.println( grade1 >= 60 ? "Passed":"Failed");
		
		System.out.println("Please enter a number:");
		int grade2 = scannerObjec.nextInt();
		System.out.println( grade2 >= 60 ? "Passed":"Failed");
		

		
		/*String a = new String("java");//new =>跟記憶體要一塊空間
